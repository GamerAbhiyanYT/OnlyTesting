local DarkraiX = loadstring(game:HttpGet("https://raw.githubusercontent.com/GamingScripter/Kavo-Ui/main/Darkrai%20Ui", true))()

local Library = DarkraiX:Window("ExplanHUB","","",Enum.KeyCode.Q);

Tab1 = Library:Tab("Recommended")

Tab2 = Library:Tab("Home")

Tab1:Seperator("If You Die And Lose Toggle")

Tab1:Line()

Tab1:Seperator("Press This Button To Bring Up Toggle Back")

Tab1:Line()

Tab1:Button("Bring Up The Toggle",function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/GamerAbhiyanYT/Explan/main/Toggle"))()
end)

Tab1:Seperator("I Still Recommend To Always Press This After Executing This Script")

Tab1:Line()

Tab1:Button("Rejoin Botton On Disconnetced",function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/rouxhaver/scripts-2/main/rejoin%20button%20for%20kick%20message.lua"))()
end)

Tab1:Toggle("Toggle",false,function(state)
if state then
print("yo") -- Script Executes When The Toggle/Switch Is On!
else
print("not yo :(")
end)

Tab1:Slider("Slider",1,100,25,function(value)
        print(value) -- Script Executes When The Slider Moves
end)

Tab1:Dropdown("Dropdown",{"yo","sus","pro"},function(value)
            print(value) Selected
end)

Tab1:Textbox("Textbox","",true,function(value)
print(value)
end)

Tab1:Seperator("A Seperator")

Tab1:Line() -- Just A Line!

DarkraiX:ToggleUi(Q)
